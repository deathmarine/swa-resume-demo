@code {
    [CascadingParameter]
    MudDialogInstance MudDialog {
        get; set;
    }
    [Parameter]
    public HashSet<string> Message {
        get; set;
    }

    void Accept() => MudDialog.Close(DialogResult.Ok(true));
    void Disable() => MudDialog.Cancel();
}
<MudDialog>
    <DialogContent>
        @foreach(var item in Message) {
            <MudText Typo="Typo.body1">@item</MudText>
        }
    </DialogContent>
    <DialogActions>
        <MudButton Color="Color.Primary" OnClick="Accept">Accept</MudButton>
        <MudButton OnClick="Disable">Disable</MudButton>
    </DialogActions>
</MudDialog>